{
  "compilerOptions": {
    "target": "ES2017",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "rootDir": "src",
    "jsx": "preserve",
    "sourceMap": true,
    "lib": [
      "esnext",
      "es2015.promise",
      "dom"
    ],
    "baseUrl": "."
  },
  "include": [
    "src"
  ],
  "files": [
    "./node_modules/@material-ui/core/index.d.ts"
  ],
  "ts-node": {
    "transpileOnly": true
  }
}
